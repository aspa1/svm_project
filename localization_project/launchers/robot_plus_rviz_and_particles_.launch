<launch>

	<include file="$(find stdr_robot)/launch/robot_manager.launch" />

	<node type="stdr_server_node" pkg="stdr_server" name="stdr_server" output="screen" args="$(find localization_project)/resources/maps/robocup.yaml"/>

	<node pkg="tf" type="static_transform_publisher" name="world2map" args="0 0 0 0 0 0  world map 100" />

	<include file="$(find stdr_gui)/launch/stdr_gui.launch" />

	<node pkg="stdr_robot" type="robot_handler" name="$(anon robot_spawn)" args="add $(find localization_project)/resources/robots/nao1.yaml 13.35 13.35 4.8" />
	<!-- node pkg="stdr_robot" type="robot_handler" name="$(anon robot_spawn)" args="add $(find localization_project)/resources/robots/robot3.yaml 15 15 0" -->

	<node pkg="localization_project" type="localization" name="localization" output="screen" />

	<!-- node pkg="rosservice" type="rosservice" name="particles" args="call wait /particles_initialization" -->

	<rosparam file="$(find localization_project)/cfg/params.yaml" command="load" />

	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find localization_project)/cfg/particles.rviz" />

</launch>
